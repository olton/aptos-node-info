div#traffic_block( data-role="panel" data-title-caption="TRAFFIC" )
    div.w-100
        div.row.border-bottom.bd-system.pb-2
            div.cell-6
                div.text-bold.text-center
                    div.reduce-3 INBOUND
                    div.reduce-5 HEALTH
                    div.text-leader2.text-bold.value#network_app_inbound_traffic_sum_HealthCheckerRpc 0
            div.cell-6.border-left.bd-system
                div.text-bold.text-center
                    div.reduce-3 OUTBOUND
                    div.reduce-5 HEALTH
                    div.text-leader2.text-bold.value#network_app_outbound_traffic_sum_HealthCheckerRpc 0

        div.row.border-bottom.bd-system.pb-2
            div.cell-6
                div.text-bold.text-center
                    div.reduce-3 INBOUND
                    div.reduce-5 STORAGE
                    div.text-leader2.text-bold.value#network_app_inbound_traffic_sum_StorageServiceRpc 0
            div.cell-6.border-left.bd-system
                div.text-bold.text-center
                    div.reduce-3 OUTBOUND
                    div.reduce-5 STORAGE
                    div.text-leader2.text-bold.value#network_app_outbound_traffic_sum_StorageServiceRpc 0

